/* pages/aboutus/aboutus.wxss */
page {
    background-color:#eee;
}
.about_box {
    width: 90%;
    height: auto;
    margin: 0 auto;
}
.about_logo {
    width: 50%;
    height: 140px;
    margin: 0 auto;
    margin-top: 20px;
}
.about_li {
    width: 100%;
    height: 50px;
    line-height: 50px;
    font-family: "Microsoft Yahei";
    font-size: 14px;
    color: #000;
    margin-top: 1px;
    background-color: #fff;
}
.about_li:active {
    background-color:#dedbdb;
}
.about_li_area {
    width: 90%;
    height: 50px;
    margin: 0 auto;
}
.about_link {
    width: 140px;
    height: 20px;
    position: fixed;
    bottom: 10px;
    border: 1px solid #818181;
    border-radius: 30px;
    line-height: 20px;
    font-family: "Microsoft Yahei";
    font-size: 12px;
    color: #818181;
    margin-left: 27%;
    overflow:hidden;
}
.about_link_left {
    width: 70px;
    height: 20px;
    float:left;
    text-align:center;
    transition: all 458ms;
}
.about_link_left:active {
    background-color: #00a1e9;
    color: #fff;
}
.about_link_right {
    width: 69px;
    height: 20px;
    float:right;
    border-left: 1px solid #818181;
    text-align:center;
    transition: all 458ms;
}
.about_link_right:active {
    background-color: #00a1e9;
    color: #fff;
}
.about_img {
    width: 100%;
    height: 100%;
}