/* pages/examresults/examresults.wxss */
page {
    background-color:#eee;
    padding-top: 20px;
}
.es_box {
    width: 90%;
    height: auto;
    padding-bottom: 15px;
    padding-top: 15px;
    background-color: #fff;
    margin: 0 auto;
    margin-bottom: 80px;
}
.es_area {
    width: 92%;
    height: auto;
    margin: 0 auto;
}
.es_examname {
    width: 100%;
    height: 20px;
    line-height: 20px;
    text-align: center;
    font-family: "Microsoft Yahei";
    font-size: 17px;
    color: #000;
}
.es_examinfo {
    width: 100%;
    height: 40px;
    line-height: 40px;
    text-align: center;
    font-family: "Microsoft Yahei";
    font-size: 12px;
    color: #000;
}
.es_font_a {
     font-family: "Microsoft Yahei";
     font-size: 18px;
     color: #09b726;
}
.es_examlist {
    width: 100%;
    height: auto;
    overflow: hidden;
}
.es_el_libox {
    width: 25%;
    height: 65px;
    float: left;
}
.es_el_li {
    width: 90%;
    height: 60px;
    margin: 0 auto;
    font-family: "Microsoft Yahei";
    font-size: 20px;
    color: #fff;
    text-align:center;
    line-height: 60px;
    opacity: 0.8;
}
.es_el_li:active {
    opacity: 1;
}

.es_menu_box {
    width: 100%;
    height: 50px;
    line-height: 50px;
    background-color: #fff;
    position: fixed;
    bottom: 0px;
}
.es_menu_li {
    width: 25%;
    height: 50px;
    float: left;
    border-right: 1px solid #eee;
    margin-left: -1px;
    font-family: "Microsoft Yahei";
    font-size: 15px;
    color: #000;
    text-align:center;
}
.es_menu_li:active {
    background-color: #eee;
}